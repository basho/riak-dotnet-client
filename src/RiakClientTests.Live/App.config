<?xml version="1.0" encoding="utf-8"?>
<configuration>
  
  <configSections>
    <section name="riak1NodeConfiguration" type="Riak.Config.ClusterConfiguration, RiakClient"/>
    <section name="riak1NodeNoAuthConfiguration" type="Riak.Config.ClusterConfiguration, RiakClient"/>
    <section name="riakShortConnectConfiguration" type="Riak.Config.ClusterConfiguration, RiakClient"/>
    <section name="riakShortWriteConfiguration" type="Riak.Config.ClusterConfiguration, RiakClient"/>
    <section name="riakShortReadConfiguration" type="Riak.Config.ClusterConfiguration, RiakClient"/>
    <section name="riakDevrelConfiguration" type="Riak.Config.ClusterConfiguration, RiakClient"/>
    <section name="riakDevrelNoAuthConfiguration" type="Riak.Config.ClusterConfiguration, RiakClient"/>
  </configSections>

  <riak1NodeConfiguration nodePollTime="5000" defaultRetryWaitTime="200" defaultRetryCount="3">
    <authentication username="riakuser" certificateAuthorityFile="..\..\..\..\tools\test-ca\certs\cacert.pem" clientCertificateFile="..\..\..\..\tools\test-ca\certs\riakuser-client-cert.pfx" clientCertificateSubject="E=riakuser@myorg.com, CN=riakuser, OU=Development, O=Basho Technologies, S=WA, C=US" checkCertificateRevocation="false"/>
    <nodes>
      <node name="dev1" hostAddress="riak-test" pbcPort="10017" poolSize="32" networkReadTimeout="60000" networkWriteTimeout="60000"/>
    </nodes>
  </riak1NodeConfiguration>

  <riak1NodeNoAuthConfiguration nodePollTime="5000" defaultRetryWaitTime="200" defaultRetryCount="3">
    <nodes>
      <node name="dev1" hostAddress="riak-test" pbcPort="10017" poolSize="32" networkReadTimeout="60000" networkWriteTimeout="60000"/>
