      <node name="dev4" hostAddress="riak-test" pbcPort="10047" poolSize="32" networkReadTimeout="60000" networkWriteTimeout="60000"/>
    </nodes>
  </riakDevrelConfiguration>

  <riakDevrelNoAuthConfiguration nodePollTime="5000" defaultRetryWaitTime="200" defaultRetryCount="3">
